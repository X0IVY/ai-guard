# AI Guard Development Environment
# Copy this file to .env.local and update values as needed

# Browser Extension Configuration
# Target URLs for content script injection
VITE_TARGET_URLS=https://chat.openai.com/*,https://claude.ai/*,https://gemini.google.com/*

# Development Settings
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug

# Extension Settings
VITE_EXTENSION_NAME=AI Guard
VITE_EXTENSION_VERSION=0.1.0

# Tracking Sensitivity (1-10, higher = more sensitive)
VITE_TRACKING_SENSITIVITY=7

# Dashboard Update Interval (ms)
VITE_DASHBOARD_UPDATE_INTERVAL=500
